extends ../ProtoBase

append content
  .container
    .row.text-center
      //- send to fragment overview page
      a(href=render_vars.fragment.overview_url).btn.btn-primary.btn-lg.my-2 work fragment
    .row.my-2
      each props, street in streets
        .col-sm
          .card
            .card-header= street
            ul.list-group.list-group-flush
              each v, i in props.blocks
                li.list-group-item #{v.hundred} #{v.odd_even}
