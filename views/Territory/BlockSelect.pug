extends ../ProtoBase 

append content
  .container.my-2
    nav(aria-label='breadcrumb')
      ol.breadcrumb
        li.breadcrumb-item.active(aria-current="page") Fragment #{render_vars.fragment.number}
        li.breadcrumb-item  Block
        li.breadcrumb-item Unit

  .container
    ul
      each block in blocks
        li
          a(href=block.overview_url) #{block.hundred} #{block.street} (#{block.odd_even})

        
